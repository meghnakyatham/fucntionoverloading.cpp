#include <iostream>
using namespace std;
 //These are three overloaded functions with all three same names but different argument list
int add(int a, int b){ //Number of arguments is 2, and the data type is integer
    return a+b;
}
float add(float a, float b){ //Number of arguments is 2 and the data type is float
    return a+b;
}
int add(int a, int b, int c){ //Number of arguments is 3 and the data type is integer
    return a+b+c;
}

int main(){

     cout<<add(12,13)<<endl;
     cout<<add(33.5f,22.4f)<<endl;
     cout<<add(99,45,76)<<endl;
    return 0;
}
